{
  "name": "caviar",
  "version": "2.0.1",
  "description": "A pluggable and extendable web framework which integrates React, Next and Koa to create your own web application out of the box with even zero configuration",
  "main": "src/index.js",
  "scripts": {
    "test": "nyc ava --timeout=10s --verbose",
    "test:only": "NODE_DEBUG=caviar nyc ava --timeout=10s --verbose",
    "lint": "eslint .",
    "posttest": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "report:dev": "nyc report --reporter=html && npm run report:open",
    "report:open": "open coverage/index.html",
    "test:dev": "npm run test && npm run report:dev && rm -rf node_modules/.cache"
  },
  "files": [
    "src/",
    "spawner/"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kaelzhang/caviar.git"
  },
  "keywords": [
    "caviar",
    "roe cli",
    "next",
    "webpack"
  ],
  "engines": {
    "node": ">= 6"
  },
  "ava": {
    "babel": false,
    "files": [
      "test/*.test.js"
    ]
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/caviar/issues"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "codecov": "^3.3.0",
    "eslint": "^5.16.0",
    "eslint-config-ostai": "^3.0.0",
    "eslint-plugin-import": "^2.17.2",
    "fs-extra": "^7.0.1",
    "get-port": "^5.0.0",
    "nyc": "^14.0.0",
    "react": "^16.8.6",
    "request": "^2.88.0",
    "roe-define-router": "^1.0.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.4.3",
    "core-util-is": "^1.0.2",
    "cross-spawn": "^6.0.5",
    "dotenv": "^7.0.0",
    "egg-serve-static": "^2.1.1",
    "env-to-code": "^1.0.4",
    "err-object": "^4.2.0",
    "express-to-koa": "^1.1.4",
    "has-own-prop": "^1.0.1",
    "mix2": "^1.0.3",
    "next": "^8.1.0",
    "next-compose-plugins": "^2.1.1",
    "once": "^1.4.0",
    "progress-hooks": "^2.3.1",
    "promise.extra": "^4.0.0",
    "react-dom": "^16.8.6",
    "roe": "^2.0.0",
    "tapable": "^1.1.3",
    "webpack": "^4.30.0"
  }
}
