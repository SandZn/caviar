{
  "name": "roe-scripts",
  "version": "1.0.1",
  "description": "Command line tools for roe",
  "main": "src/index.js",
  "bin": {
    "roe": "bin/roe.js"
  },
  "scripts": {
    "build:node": "babel stage --out-dir src",
    "test": "nyc ava --timeout=10s",
    "test-no-report": "NODE_DEBUG=roe-scripts nyc ava --timeout=10s --verbose",
    "lint": "eslint .",
    "posttest": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "prepublishOnly": "npm run build:node"
  },
  "files": [
    "bin/",
    "cli/",
    "src/",
    "spawner/"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kaelzhang/roe-scripts.git"
  },
  "keywords": [
    "roe-scripts",
    "roe cli",
    "next",
    "webpack"
  ],
  "engines": {
    "node": ">= 6"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/roe-scripts/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/register": "^7.4.0",
    "ava": "^1.4.1",
    "babel-eslint": "^10.0.1",
    "codecov": "^3.3.0",
    "eslint": "^5.16.0",
    "eslint-config-ostai": "^3.0.0",
    "eslint-plugin-import": "^2.16.0",
    "nyc": "^13.3.0"
  },
  "dependencies": {
    "after-ready": "^1.0.8",
    "common-bin": "^2.8.1",
    "core-util-is": "^1.0.2",
    "cross-spawn": "^6.0.5",
    "dotenv": "^7.0.0",
    "egg-serve-static": "^2.1.1",
    "env-to-code": "^1.0.4",
    "err-object": "^4.1.0",
    "express-to-koa": "^1.1.4",
    "mz": "^2.7.0",
    "next": "^8.0.4",
    "progress-hooks": "^2.0.0",
    "promise.extra": "^4.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "roe": "^1.1.2",
    "tapable": "^1.1.1",
    "webpack": "^4.29.6"
  }
}
